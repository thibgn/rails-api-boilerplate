meta {
  name: Signup
  type: http
  seq: 1
}

post {
  url: http://127.0.0.1:3000/signup
  body: json
}

assert {
  res.status: eq 200
}

script:post-response {
  bru.setVar('user_id', res.body.id)
}

script:pre-request {
  const { faker } = require('@faker-js/faker')


  req.setBody({
    user: {
      email: faker.internet.email(),
      password: "password"
    } 
  })
}
